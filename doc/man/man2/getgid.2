.SH NAME
.\"
getgid \- Gets the real user group ID of the calling process.
.\"
.\"=============================================================================
.\"
.\"
.SH "SYNOPSIS"
.\"
.IR "#include <unistd.h>"

.BI "gid_t getgid(void);"
.\"
.\"=============================================================================
.\"
.SH "DESCRIPTION"
.\"
The
.BR getgid()
system call returns the real user group ID of the calling process.
.\"
.\"=============================================================================
.\"
.SH "RETURN VALUE"
.\"
The
.BR getgid()
system call is always successful and no return value is reserved to indicate an
error.
.\"
.\"=============================================================================
.\"
.SH ERRORS
.\"
No errors are defined.
.\"
.\"=============================================================================
.\"
.SH "CONFORMING TO"
.\"
POSIX.1-2001
